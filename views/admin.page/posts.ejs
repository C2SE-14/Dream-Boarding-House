<h2>Posts</h2>

<table class="table table-hover">
  <thead>
    <tr class="text-center">
      <th scope="col">#</th>
      <th scope="col">Username</th>
      <th scope="col">Email</th>
      <th scope="col">Phone Number</th>
      <th scope="col">Actions</th>
    </tr>
  </thead>
  <tbody>
    <% roleOwnersOfRoom.forEach((owner, index) => {%>
    <tr class="text-center">
      <th scope="row"><%= index+1 %></th>
      <% users.forEach(user => {%> <% if(owner.userId === user.id) { %>
      <td><%= user.username %></td>
      <td><%= user.email %></td>
      <td><%= user.phoneNumber %></td>
      <% } %> <% }) %>
      <td>
        <a class="btn btn-primary" href="/admin/owner/<%= owner.userId %>"
          >Room</a
        >
      </td>
    </tr>
    <% }) %>
  </tbody>
</table>
